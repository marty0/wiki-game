@{
    Layout = null;
}

@Html.CSS("wiki-styles.css")
@Html.CSS("Site.css")
@Html.CSS("bootstrap.min.css")

<div id="won">
    @if (ViewBag.hasWon)
    {
        <div class="alert alert-success" role="alert">
            <p>You Won!</p>
            <p>Your score is @ViewBag.moves</p>
            <p>Your rank is @ViewBag.position</p>
        </div>
    }
</div>
<div id="wiki_page">
    @ViewBag.wiki_page
</div>
